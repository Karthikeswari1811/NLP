      "source": [
        "def tokenize_text(text):\n",
        "    tokens = text.split()\n",
        "    return tokens\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    text = \"Natural Language Processing is fun\"\n",
        "\n",
        "    tokens = tokenize_text(text)\n",
        "\n",
        "    print(\"Tokens:\")\n",
        "    print(tokens)"
      ]
